# Frontend environment variables for Netlify
NEXT_PUBLIC_API_URL=https://your-netlify-site.netlify.app
NEXT_PUBLIC_APP_URL=https://your-netlify-site.netlify.app

# OAuth callback URLs (update with your Netlify domain)
NEXT_PUBLIC_GOOGLE_CALLBACK_URL=https://your-netlify-site.netlify.app/auth/google/callback
NEXT_PUBLIC_TWITTER_CALLBACK_URL=https://your-netlify-site.netlify.app/auth/twitter/callback
NEXT_PUBLIC_FACEBOOK_CALLBACK_URL=https://your-netlify-site.netlify.app/auth/facebook/callback
NEXT_PUBLIC_INSTAGRAM_CALLBACK_URL=https://your-netlify-site.netlify.app/auth/instagram/callback
